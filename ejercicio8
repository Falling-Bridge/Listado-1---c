#include <stdio.h>

int main(){
    int n, i, m;
    printf("ingrese el valor de m:"); scanf("%d", &m); printf("ingrese el valor de n:"); scanf("%d", &n);
    int arreglo[n];
    for(i=0 ; i< n; i++){
        int aux = i+1;
        arreglo[i]=aux;
        printf("%d \n", arreglo[i]%m);
    }
    return 0;
}
