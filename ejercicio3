#include <stdio.h>

int main(){
    int n, i, m, u;
    printf("ingrese la cantidad de numeros: \n"); scanf("%d", &n);
    printf("ingrese el valor de m: \n"); scanf("%d", &m);
    int arreglo[n];
    for(i = 0; i < n; i++){
        printf("ingrese un numero:\n"); scanf("%d", &u); arreglo[i] = u; 
    }
    printf("Los numeros mayores a m son: ");
    for (i = 0; i < n; i++) {
        if (m < arreglo[i]) {
            printf("%d ", arreglo[i]);
        }
    }
    return 0;
}
